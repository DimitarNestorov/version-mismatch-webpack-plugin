{
  "name": "version-mismatch-webpack-plugin",
  "version": "0.0.1",
  "description": "Checks for differences between the installed versions of dependencies and the versions specified in package.json",
  "main": "index.js",
  "scripts": {
    "debug": "DEBUG=version-mismatch-webpack-plugin node --inspect-brk=9229 ./node_modules/.bin/webpack",
    "start": "DEBUG=version-mismatch-webpack-plugin webpack",
    "watch": "DEBUG=version-mismatch-webpack-plugin webpack --watch",
    "prepublishOnly": "npm i; yarn"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimitarnestorov/version-mismatch-webpack-plugin.git"
  },
  "keywords": [
    "version",
    "mismatch",
    "webpack",
    "dependencies"
  ],
  "author": {
    "name": "Dimitar Nestorov",
    "email": "opensource@dimitarnestorov.com",
    "url": "https://dimitarnestorov.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dimitarnestorov/version-mismatch-webpack-plugin/issues"
  },
  "homepage": "https://github.com/dimitarnestorov/version-mismatch-webpack-plugin#readme",
  "dependencies": {
    "debug": "^3.1.0",
    "semver": "^5.5.0"
  },
  "devDependencies": {
    "ajv": "^6.2.0",
    "eslint": "^4.18.1",
    "eslint-config-dimitarnestorov": "^1.1.1",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.9"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
